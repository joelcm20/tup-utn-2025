Proceso ejercicio4_1
	Definir resultado,a,b,c Como Real;
	
	Escribir "Asignar valor a A: "; // asignar 2
	Leer a;
	Escribir "Asignar valor a B: "; // asignar 10
	Leer b;
	Escribir "Asignar valor a C: "; // asignar 3
	Leer c;
	
	resultado<-(-b + rc(b^2 - 4 * a * c)) / (2*a);
	Escribir "El primer resultado es: ", resultado;
	
	resultado<-(-b - rc(b^2 - 4 * a * c)) / (2*a);
	Escribir "El segundo resultado es: ", resultado;
FinProceso
