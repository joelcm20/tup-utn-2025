Proceso ejercicio_3
	// comentario
	Definir a, b, resultado Como Real;
	// asignar un numero a "a"
	a <- 45;
	Escribir 'Digite un numero 'Sin Saltar;
	Leer b;
	// suma de "a" y "b" dividida por la multiplicacion de "a" por "b",
	// luego almacenamos el valor en "resultado"
	resultado <- (a+b)/(a*b);
	Escribir 'El resultado es: ', resultado;
FinProceso
